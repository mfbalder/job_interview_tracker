<div id="<%= dom_id interview %>">
    <p><span class='job_header'><%= interview.date.strftime("%B %e") %> at <%= interview.start_time.strftime("%l:%M%P") %>, with <%= interview.interviewer_name %> </span></p>
    <p><span class='job_header'>Questions to Ask: </span><%= interview.questions_to_ask %></p>
    <p><span class='job_header'>Notes: </span><%= interview.notes %></p>
    <p><span class='job_header'>Reflections: </span><%= interview.reflections %></p>
</div>
<div>
    <%= link_to "Edit this interview", edit_job_interview_path(@job, interview) %>
</div>
